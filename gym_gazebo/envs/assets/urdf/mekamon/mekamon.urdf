    <origin rpy="0 0 3.926875" xyz="-0.04 -0.04 -0.025"/>
  </joint>
  <!-- Hip, containing servo -->
  <link name="br_hip">
    <visual>
      <geometry>
        <!--length, width, height-->
        <box size="0.04 0.03 0.03"/>
      </geometry>
      <!-- Where we want the link centre to be, relative to the joint? -->
      <origin rpy="0 0 0" xyz="0.02 0 0"/>
      <material name="hip"/>
    </visual>
    <collision>
      <!--same as visual geometry -->
      <geometry>
        <box size="0.04 0.03 0.03"/>
      </geometry>
      <!--same as visual geometry -->
      <origin rpy="0 0 0" xyz="0.02 0 0"/>
    </collision>
    <inertial>
      <mass value="0.02"/>
      <inertia ixx="2.9999988e-06" ixy="0.0" ixz="0.0" iyy="4.166665e-06" iyz="0.0" izz="4.166665e-06"/>
    </inertial>
  </link>
  <!-- hip-to-hiplink joint -->
  <joint name="br_hip_joint" type="revolute">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-1.04716666667" upper="1.04716666667" velocity="0.5"/>
    <!-- Where this joint attaches to the parent link,
      relative to previous joint origin -->
    <origin rpy="0 0 0" xyz="0.04 0 0"/>
    <parent link="br_hip"/>
    <child link="br_hiplink"/>
  </joint>
  <!-- Hiplink - small passive right-angle link -->
  <link name="br_hiplink">
    <visual>
      <geometry>
        <box size="0.03 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.015 0 0"/>
      <material name="hiplink"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.03 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.015 0 0"/>
    </collision>
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="1.4999994e-06" ixy="0.0" ixz="0.0" iyy="1.4999994e-06" iyz="0.0" izz="1.4999994e-06"/>
    </inertial>
  </link>
  <!-- hiplink-to-thigh joint -->
  <joint name="br_hiplink_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.396" upper="1.484" velocity="0.5"/>
    <!-- Where this joint attaches to the parent link,
      relative to previous joint origin -->
    <origin rpy="0 0 0" xyz="0.03 0 0"/>
    <parent link="br_hiplink"/>
    <child link="br_thigh"/>
  </joint>
  <!-- Thigh -->
  <link name="br_thigh">
    <visual>
      <geometry>
        <box size="0.09 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.045 0 0"/>
      <material name="thigh"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.09 0.03 0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.045 0 0"/>
    </collision>
    <inertial>
      <mass value="0.05"/>
      <inertia ixx="7.499997e-06" ixy="0.0" ixz="0.0" iyy="3.7499985e-05" iyz="0.0" izz="3.7499985e-05"/>
    </inertial>
  </link>
  <!-- Knee, thigh-to-foreleg joint -->
  <joint name="br_knee_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.169" upper="1.274" velocity="0.5"/>
    <!-- Where this joint attaches to the parent link,
      relative to previous joint origin -->
    <origin rpy="0 0 0" xyz="0.09 0 0"/>
    <parent link="br_thigh"/>
    <child link="br_foreleg"/>
  </joint>
  <!-- Foreleg -->
  <link name="br_foreleg">
    <visual>
      <geometry>
        <box size="0.105 0.03 0.03"/>
      </geometry>
      <!-- translation of origin is relative to pre-rotation frame,
        so x-trans is done as z-trans-->
      <origin rpy="0 1.57075 0" xyz="0 0 -0.0525"/>
      <material name="foreleg"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.105 0.03 0.03"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 -0.0525"/>
    </collision>
    <inertial>
      <mass value="0.03"/>
      <inertia ixx="4.4999982e-06" ixy="0.0" ixz="0.0" iyy="2.9812488075e-05" iyz="0.0" izz="2.9812488075e-05"/>
    </inertial>
  </link>
  <!-- Foreleg to Foot static joint -->
  <joint name="br_ankle" type="fixed">
    <parent link="br_foreleg"/>
    <child link="br_foot"/>
    <!-- Where this joint attaches to the parent link,
      relative to previous joint origin -->
    <origin rpy="0 0 0" xyz="0 0 -0.105"/>
  </joint>
  <!-- Foot -->
  <link name="br_foot">
    <visual>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="foot"/>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="1.6e-06" ixy="0.0" ixz="0.0" iyy="1.6e-06" iyz="0.0" izz="1.6e-06"/>
    </inertial>
  </link>
  <transmission name="br_hip_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="br_hip_joint">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="br_hip_joint_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  <transmission name="br_hiplink_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="br_hiplink_joint">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="br_hiplink_joint_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  <transmission name="br_knee_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="br_knee_joint">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="br_knee_joint_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
</robot>
